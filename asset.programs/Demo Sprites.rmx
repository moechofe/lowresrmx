
dim speed(169,1)

for i=0 to ubound(speed,1)
  speed(i,0)=(rnd(7)+1)/8
  speed(i,1)=(rnd(7)+1)/8
  sprite i,rnd(shown.w-50),rnd(shown.h-50),1
  sprite i size 3
  sprite i pal rnd(7)
next i

do
  for i=0 to ubound(speed,1)
    sprite i,sprite.x(i)+speed(i,0),sprite.y(i)+speed(i,1),
    if sprite.x(i)>=shown.w-26 and speed(i,0)>0 then speed(i,0)=-speed(i,0)
    if sprite.x(i)<=0 and speed(i,0)<0 then speed(i,0)=-speed(i,0)
    if sprite.y(i)>=shown.h-26 and speed(i,1)>0 then speed(i,1)=-speed(i,1)
    if sprite.y(i)<=0 and speed(i,1)<0 then speed(i,1)=-speed(i,1)
  next i
  wait vbl
loop


