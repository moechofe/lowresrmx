sprite 0 size 3 pal 1
sprite 1 size 3 pal 0

x=0
s=1/60

wx=ceil(safe.l/8)+2
wy=ceil(safe.t/8)+2
ww=floor((shown.w-safe.l-safe.r)/8)-4
wh=floor((shown.h-safe.t-safe.b)/8)-4

window wx,wy,ww,wh,0
prio 1

print "linear"
print "sine"
print "quad"
print "cubic"
print "quart"
print "quint"
print "circ"
print "back"
print "elastic"
print "bounce"
print
print "in"
print "inout"
print "out"

f=0
m=0

do
  fx=ease(f,m,x)
  y=fx*(shown.h-32-80)+40
  sprite 0,30,y,64

  text wx+8,wy+0,"  x ="+left$(str$(x),5)
  text wx+8,wy+2,"f(x)="+left$(str$(fx),5)

  c=clamp(int(fx*3.4),0,3)*4
  sprite 1,30,shown.h-48,c

  add x,abs(s)
  if x>=1 then x=0

  if tap then
    tx=int(touch.x/8)-wx
    ty=int(touch.y/8)-wy
    if tx>=0 and tx<=7 and ty>=0 and ty<=9 then
      bg tint 0,wy+f to 7,wy+f pal 0
      f=ty
    else if tx>=0 and tx<=7 and ty>=11 and ty<=13 then
      bg tint 0,wy+12+m to 8,wy+12+m pal 0
      m=ty-12
    end if

  end if

  bg tint 0,wy+f to 8,wy+f pal 1
  bg tint 0,wy+12+m to 8,wy+12+m pal 1

  wait vbl
loop


