{
    "cmake.sourceDirectory": "${workspaceFolder}/project.cmake",
    "cmake.buildDirectory": "${workspaceFolder}/project.cmake/build",
    "cmake.configureArgs": [
        "-DSDL=./SDL2-2.30.0"
    ],
    "files.associations": {
        "*.embeddedhtml": "html",
        "*.tcc": "c",
        "compare": "c",
        "runner.h": "c",
        "data_manager.h": "c",
        "value.h": "c",
        "audio_lib.h": "c",
        "cctype": "c",
        "format": "c",
        "charconv": "c",
        "ratio": "c",
        "array": "c",
        "functional": "c",
        "tuple": "c",
        "type_traits": "c",
        "utility": "c",
        "system_error": "c",
        "chrono": "c",
        "optional": "c",
        "variant": "c",
        "math.h": "c",
        "context.h": "c",
        "globmatch.h": "c"
    },
    "cmake.configureOnOpen": true,
    "files.watcherExclude": {
        "**/.stversions": true,
        "**/*.bc": true,
        "**/*.stfolder": true
    },
    "search.exclude": {
        "**/.stfolder": true,
        "**/.stversions": true,
        "**/*.bc": true,
        "project.flutter/core_plugin/src/core.*": true
    },
    "files.exclude": {
        "**/.stfolder": true,
        "**/.stversions": true,
        "**/*.bc": true
    },
    "C_Cpp.files.exclude": {
        "**/.stfolder": true,
        "**/.stversions": true,
        "**/*.bc": true,
        "**/.git": true,
        "**/.vscode": true,
        "**/.vs": true
    }
}
